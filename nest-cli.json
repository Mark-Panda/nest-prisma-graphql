{
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/account/src",
    "compilerOptions": {
        "webpack": true,
        "tsConfigPath": "apps/account/tsconfig.app.json"
    },
    "projects": {
        "account": {
            "type": "application",
            "root": "apps/account",
            "entryFile": "main",
            "sourceRoot": "apps/account/src",
            "compilerOptions": {
                "tsConfigPath": "apps/account/tsconfig.app.json"
            }
        },
        "public-tool": {
            "type": "library",
            "root": "libs/public-tool",
            "entryFile": "index",
            "sourceRoot": "libs/public-tool/src",
            "compilerOptions": {
                "tsConfigPath": "libs/public-tool/tsconfig.lib.json"
            }
        },
        "public-module": {
            "type": "library",
            "root": "libs/public-module",
            "entryFile": "index",
            "sourceRoot": "libs/public-module/src",
            "compilerOptions": {
                "tsConfigPath": "libs/public-module/tsconfig.lib.json"
            }
        }
    }
}